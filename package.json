{
	"name": "extension-for-graphics",
	"version": "1.0.2",
	"private": true,
	"description": "Prolo browser extension for Graphics",
	"author": "Technology - Graphic HTML",
	"type": "module",
	"scripts": {
		"build": "wxt build",
		"build:firefox": "wxt build -b firefox",
		"compile": "tsc --noEmit",
		"dev": "wxt",
		"dev:firefox": "wxt -b firefox",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"postinstall": "wxt prepare",
		"zip": "wxt zip",
		"zip:firefox": "wxt zip -b firefox"
	},
	"dependencies": {
		"jszip": "^3.10.1",
		"papaparse": "^5.5.3",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-loader-spinner": "^6.1.6",
		"react-spinners": "^0.17.0",
		"sweetalert": "^2.1.2",
		"sweetalert2": "^11.23.0"
	},
	"devDependencies": {
		"@stacksjs/bumpx": "0.1.82",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.3",
		"@wxt-dev/module-react": "^1.1.3",
		"prettier": "^3.6.2",
		"sass-embedded": "^1.91.0",
		"typescript": "^5.8.3",
		"wxt": "^0.20.6"
	}
}
